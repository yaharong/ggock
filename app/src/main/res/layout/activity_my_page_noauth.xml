<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.king.togi.ggock.ui.MyPageNoAuthActivity">

    <!--<android.support.design.widget.AppBarLayout-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="wrap_content"-->
    <!--android:theme="@style/AppTheme.AppBarOverlay">-->

    <!--</android.support.design.widget.AppBarLayout>-->

    <!--<include layout="@layout/content_my_page_noauth" />-->

    <TextView
        android:id="@+id/textView2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="hello"
        android:layout_marginTop="78dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/kakaoBtn"
            android:layout_width="300dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:text="카카오톡"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="240dp" />

        <Button
            android:id="@+id/faceBtn"
            android:layout_width="300dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:text="페이스북"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/kakaoBtn"
            android:layout_alignLeft="@+id/kakaoBtn"
            android:layout_alignStart="@+id/kakaoBtn" />

        <Button
            android:id="@+id/navBtn"
            android:layout_width="300dp"
            android:layout_height="40dp"
            android:layout_gravity="bottom|center"
            android:gravity="center_horizontal"
            android:text="네이버"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/faceBtn"
            android:layout_alignLeft="@+id/faceBtn"
            android:layout_alignStart="@+id/faceBtn" />

        <TextView
            android:id="@+id/회원가입"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navBtn"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:text="회원가입" />


    </RelativeLayout>

</RelativeLayout>
